<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title</title>
    @System.Web.Optimization.Styles.Render("~/Content/Default")
    @System.Web.Optimization.Styles.Render("~/Content/Supervision")
    @System.Web.Optimization.Scripts.Render("~/bundles/jquery")
    @System.Web.Optimization.Scripts.Render("~/bundles/scripts")
    @System.Web.Optimization.Scripts.Render("~/bundles/Supervision")
</head>
<body>
    <div id="wrapper">
        <nav class="navbar navbar-default navbar-static-top" role="navigation" style="margin-bottom: 0">
            @Html.Partial("Partials/TopMenu")
            @Html.Partial("Partials/Sidebar")
        </nav>
        <div id="page-wrapper">
            @RenderBody()
        </div>
    </div>
    @Html.Partial("Partials/Component")
    @RenderSection("scripts", false)
</body>
</html>